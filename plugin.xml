<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Sample Multi-page Editor"
            extensions="gph"
            icon="icons/sample.gif"
            contributorClass="graphstreamfeasibility.editors.MultiPageEditorContributor"
            class="graphstreamfeasibility.editors.MultiPageEditor"
            id="graphstreamfeasibility.editors.MultiPageEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Sample Wizards"
            id="GraphStreamFeasibility">
      </category>
      <wizard
            name="Multi-page Editor file"
            icon="icons/sample.gif"
            category="GraphStreamFeasibility"
            class="graphstreamfeasibility.wizards.SampleNewWizard"
            id="graphstreamfeasibility.wizards.SampleNewWizard">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="visuflow.nodeView"
            name="NodeView">
      </category>
      <view
            class="de.unipaderborn.visuflow.NodeView"
            id="visuflow.views.nodeView"
            name="nodeView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="GraphStreamFeasibility.view1"
               minimized="false"
               relationship="stack">
         <view
               id="visuflow.views.nodeView1"
               minimized="false"
               relationship="stack">
         </view>
         </view>
      </perspectiveExtension>
   </extension>

</plugin>
